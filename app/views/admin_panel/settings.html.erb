<%= render 'header' %>

<div class="container-fluid page">
	<div class="row">
		<%= render partial: 'sidebar', locals: { active: 'settings' } %>
		<div class="col-sm-10">
			<div class="content">
				<%= form_for @data, url: data_save_admin_panel_url, method: :patch do |form| %>
					<%= form.fields_for :children do |child, c| %>
						<%= data_to_form_label child %>
						<%= data_to_form_field child %>
					<% end %>
				<% end %>
			</div>
		</div>
	</div>
</div>
